{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "quotes": "off",
    "import/no-extraneous-dependencies": "off",
    "no-unused-vars": "warn",
    "object-curly-newline": "warn",
    "max-len": "warn"
  }
}
